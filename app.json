{
  "expo": {
    "name": "Where-Am-I",
    "slug": "Where-Am-I",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "citymapper"
        ]
      }
    },
    "android": {
      "package": "com.acm.where_am_i"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
